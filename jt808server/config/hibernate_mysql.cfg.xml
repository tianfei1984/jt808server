		<mapping class="com.ltmonitor.entity.EnclosureBinding" />
		<mapping class="com.ltmonitor.entity.MemberInfo" />

		<mapping class="com.ltmonitor.entity.AlarmConfig" />
		<mapping class="com.ltmonitor.jt808.entity.SpeedRecorder" />
