		<mapping class="com.ltmonitor.entity.SystemConfig" />
		<mapping class="com.ltmonitor.entity.OperationLog" />
		<mapping class="com.ltmonitor.entity.EWayBill" />
		<mapping class="com.ltmonitor.entity.GovPlatformMsg" />
